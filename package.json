{
  "name": "node-dijkstra",
  "version": "2.5.0",
  "description": "A NodeJS implementation of Dijkstra's algorithm",
  "author": "Alberto Restifo <alberto.restifo@gmail.com>",
  "main": "./index.js",
  "module": "./index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/smart-parks/node-dijkstra"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js; du index.js; gzip -c index.js | wc -c",
    "format": "prettier --write {libs,test}/*.js",
    "test": "yarn jest --coverage"
  },
  "bugs": {
    "url": "https://github.com/smart-parks/node-dijkstra/issues"
  },
  "keywords": ["dijkstra", "shortest path"],
  "license": "MIT",
  "devDependencies": {
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rollup": "^2.11.2",
    "rollup-jest": "^0.0.2",
    "rollup-plugin-terser": "^6.1.0"
  },
  "jest": {
    "testMatch": ["**/test/*.test.js"],
    "transform": {
      "^.+\\.js$": "rollup-jest"
    }
  }
}
